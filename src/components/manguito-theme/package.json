{
  "name": "@bobbykim/manguito-theme",
  "version": "0.28.0",
  "description": "main theme file for manguito components library. Other MCL components will need to import this file to run.",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vue",
    "vue3",
    "components",
    "tailwindcss",
    "tailwind",
    "theme",
    "ui",
    "design-system",
    "component-library"
  ],
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "main": "./dist/core.cjs",
  "module": "./dist/core.js",
  "types": "./dist/core.d.ts",
  "exports": {
    ".": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./mcl-theme-v4.css": "./dist/mcl-theme-v4.css"
  },
  "scripts": {
    "build": "vite build && cp lib/style/mcl-theme-v4.css dist/mcl-theme-v4.css"
  },
  "dependencies": {
    "@vueuse/core": "^12.0.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "sass": "^1.82.0",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.3.0",
    "vue-tsc": "^2.1.10"
  },
  "author": {
    "name": "Bobby Kim",
    "url": "https://www.devbobbykim.com/"
  },
  "homepage": "https://manguito-component-library.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/bobbykim89/manguito-component-library.git"
  },
  "license": "MIT",
  "gitHead": "4e6b9b908a17893f6e6baab01741dc0dde771ac8"
}
